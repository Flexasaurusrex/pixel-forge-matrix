{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/storyteller/?",
      "dest": "/storyteller/index.html"
    },
    {
      "src": "/mobile/?",
      "dest": "/mobile/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
